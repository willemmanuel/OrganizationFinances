<div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <% if current_user.present? %>
            <%= link_to current_user.chapter.chapter, root_path, id: "logo" %>
          <% else %>
            <%= link_to "Omicron", root_path, id: "logo" %>
          <% end %>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
          </ul>
          <% if current_user.present? && current_user.chapter_manager %>
          <form class="navbar-form navbar-right" role="search" method="POST" action="/brother/search">
              <div class="form-group">
                <input type="text" class="form-control" placeholder="Search for brother..." name="search">
              </div>
            </form>
            <% end %>
          <ul class="nav navbar-nav navbar-right">
          <% if current_user.present? && current_user.chapter_manager %>
            <li><%= link_to "Brothers", brothers_path %></li>
            <li><%= link_to "Debts", debts_path %></li>
            <li><%= link_to "Expenses", expenses_path %></li>
            <li><%= link_to "Committees", committees_path %></li>
            <li><%= link_to "Meetings", meetings_path %></li>
            <li><%= link_to "+ Expense", new_expense_path %></li>
            <li><%= link_to "+ Debt", new_debt_path %></li>
            <li><%= link_to "+ Deposit", new_deposit_path %></li>
            <li><a href="<%= url_for manage_path %>"><i class="fa fa-cog fa-fw"></i>&nbsp;</a>  </li>
          <% elsif !current_user.present? %>
             <li><%= link_to "Sign up", new_user_registration_path %></li>
             <li><%= link_to "Log in", new_user_session_path %></li>
          <% end %>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>
